{
  "name": "wampark",
  "version": "1.6.2",
  "description": "Node.js Application Architecture for big WAMP (Web Application Message Protocol) based applications and microservices",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec babel-node --inspect=0.0.0.0:9228 --extensions .mjs,.js src",
    "dev-brk": "nodemon --exec babel-node --inspect-brk=0.0.0.0:9228 --extensions .mjs,.js src",
    "build": "babel -x .mjs,.js src -d dist --copy-files",
    "babel-version": "babel --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafael-freitas/wampark.git"
  },
  "keywords": [
    "wamp",
    "microservices",
    "application",
    "architecture",
    "crossbar",
    "nodejs"
  ],
  "author": "Rafael Freitas Lima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rafael-freitas/wampark/issues"
  },
  "homepage": "https://github.com/rafael-freitas/wampark#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.11.5"
  },
  "dependencies": {
    "autobahn": "^20.9.2",
    "cluster": "^0.7.7",
    "colors": "^1.4.0",
